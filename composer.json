{
    "name": "brandcom/cakephp-content-blocks",
    "description": "ContentBlocks plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ContentBlocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContentBlocks\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    }
}
